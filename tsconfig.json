{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es6",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["ES6", "ESNext"],
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
